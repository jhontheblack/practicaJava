    <TextView
        android:id="@+id/Rciudad"
        style="@style/RefResultados"
        android:text="Quevedo"
        android:textSize="20dp"
        android:textColor="@color/colorCuatro"
        ></TextView>

    <TextView
        style="@style/RefResultados"
        android:text="Genero">
    </TextView>

    <TextView
        android:id="@+id/Rgenero"
        style="@style/RefResultados"
        android:text="Hombre"
        android:textSize="20dp"
        android:textColor="@color/colorCuatro"
        ></TextView>

    <TextView
        style="@style/RefResultados"
        android:text="Correo">
